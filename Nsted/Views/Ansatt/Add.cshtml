@model Nsted.Models.Ansatt;

<h2>Add Ansatt</h2>

<form asp-action="Add" method="post">

    @Html.AntiForgeryToken()
    <div class="form-group">
        <label>Fornavn:</label>
        <input asp-for="Fornavn" class="form-control" required />
        <label>Etternavn:</label>
        <input asp-for="Etternavn" class="form-control" required />
    </div>

    <div class="form-group">
        <input type="submit" value="Submit" class="btn btn-primary" />
    </div>

</form>
