@model IEnumerable<Nsted.Models.Sjekkliste>

<h2>Sjekklister</h2>


<form asp-controller="Sjekkliste" asp-action="Search" method="get">
    <div class="form-group">
        <label for="searchString">SÃ¸k etter sjekklister</label>
        <input type="text" name="searchString" value="@ViewData["CurrentFilter"]" />
        <input type="submit" value="Search" class="btn btn-dark" />
    </div>
</form>

<br />

<a asp-action="List" class="btn btn-dark">Tilbake til alle sjekklister</a>

<br />
<br />

<table class="table table-bordered table-striped">
    <thead>
    <tr>
        <th>Serienummer</th>

    </tr>
    </thead>
    <tbody>
    @foreach (var sjekkliste in Model)
    {
        <tr>
            <td>@sjekkliste.Serienummer</td>
            <td>
                @Html.AntiForgeryToken()
                <a asp-action="Edit" asp-route-id="@sjekkliste.Id" class="btn btn-dark">Edit</a>
            </td>
        </tr>
    }
    </tbody>
</table>

